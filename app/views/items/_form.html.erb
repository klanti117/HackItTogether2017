<h1>New Item</h1>
<%= form_for @items, html: { multipart: true } do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>
<%= f.label :description %><br>
<%= f.text_field :description %><br>
<p>Do you want to add pictures ?</p>
<%= f.label :image %><br>
<%= f.file_field :image %><br>
<%= f.submit %>
<% end %>